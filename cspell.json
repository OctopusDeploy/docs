{
  "version": "0.2",
  "language": "en",
  "dictionaries": ["dictionary-octopus"],
  "dictionaryDefinitions": [
    {
      "name": "dictionary-octopus",
      "path": "./dictionary-octopus.txt",
      "addWords": false
    }
  ],
  "flagWords": [
    "hte->the",
    "octostash->octostache",
    "on premise->on-premises",
    "on-premise->on-premises",
    "utilize->use",
    "utilizes->uses",
    "utilized->used",
