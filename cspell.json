  "ignoreRegExpList": [
    "/\\{#(\\w|-)+\\}/g",
    "/\\([^\\)]*#[\\w-]+/g",
    "/href=\"[^\"]*#[\\w-]+/g"
  ],
  "suggestWords": [
    "allow->let",
    "allowing->letting",
    "allows->lets",
    "article->post",
    "articles->posts",
    "black list->block list",
    "blacklist->block list",
    "disable->deactivate",
    "disabled->deactivated",
    "disables->deactivates",
    "disabling->deactivating",
    "enable->let",
    "enables->lets",
    "enabling->letting",
    "leverage->use",
    "leverages->uses",
    "leveraging->using",
    "native->built-in",
    "usage->use",
    "white list->safe list",
    "white-list->safe list",
    "whitelist->safe list",
    "within->in"
  ],
  "ignorePaths": [
    "cspell.json",
    "package.json",
    "package-lock.yaml",
    "pnpm-lock.yaml",
    "docs/credits.md",
    ".octopus/**",
    ".vscode/**",
    ".github/**",
    "src/pages/report/**",
