export type RuntimePackageSelection = {
    referenceId: string;
    feedId: string | undefined; // todo-step-ui these can be bound, and it should be explicit. Change this to string | BoundValue
    packageId: string | undefined; // todo-step-ui these can be bound, and it should be explicit. Change this to string | BoundValue
    substituteInFiles?: SubstituteInFilesConfiguration | undefined;
    structuredConfigVariables?: boolean | undefined;
};

type SubstituteInFilesConfiguration = {
    targetFiles: string;
};
