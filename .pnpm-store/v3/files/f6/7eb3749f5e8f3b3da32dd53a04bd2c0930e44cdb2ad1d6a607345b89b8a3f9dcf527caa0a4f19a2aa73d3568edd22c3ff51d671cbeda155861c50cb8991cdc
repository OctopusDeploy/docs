var convertFromJsonSchemaToInputSchema_1 = require("../convertFromJsonSchemaToInputSchema");
describe("convertFromJsonSchemaToInputSchema", function () {
    test("can convert simple object properties", function () {
        var inputJsonSchema = {
            definitions: {
                MyInputs: {
                    type: "object",
                    isRoot: true,
                    properties: {
                        myProperty: { type: "string" },
                    },
                    required: ["myProperty"],
                },
            },
        };
        var inputs = {
            myProperty: "foo",
        };
        var schema = (0, convertFromJsonSchemaToInputSchema_1.convertFromJsonSchemaToInputSchema)(inputJsonSchema, inputs);
        var expectedProperty = {
            name: "myProperty",
            isRequired: true,
            type: { type: "string" },
        };
        expect(schema.properties).toEqual([expectedProperty]);
    });
    test("can convert simple object properties that aren't required", function () {
        var inputJsonSchema = {
            definitions: {
                MyInputs: {
                    type: "object",
                    isRoot: true,
                    properties: {
                        myProperty: { type: "string" },
                        mySecondProperty: { type: "string" },
                    },
                    required: ["mySecondProperty"],
                },
            },
        };
        var inputs = {
            myProperty: "foo",
        };
        var schema = (0, convertFromJsonSchemaToInputSchema_1.convertFromJsonSchemaToInputSchema)(inputJsonSchema, inputs);
        var expectedFirstProperty = {
            name: "myProperty",
            isRequired: false,
            type: { type: "string" },
        };
        var expectedSecondProperty = {
            name: "mySecondProperty",
            isRequired: true,
            type: { type: "string" },
        };
        expect(schema.properties).toEqual([expectedFirstProperty, expectedSecondProperty]);
    });
