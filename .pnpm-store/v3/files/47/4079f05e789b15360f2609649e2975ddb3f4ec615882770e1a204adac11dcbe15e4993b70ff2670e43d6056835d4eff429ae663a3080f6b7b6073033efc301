export declare function isResourceBoundValue(input: any): input is string;
export declare function asResourceInputs<StepInputs>(inputSchemaProperties: InputProperty[], runtimeInputs: ObjectRuntimeInputs<StepInputs>): ObjectResourceInputs<StepInputs>;
export declare function asStepConfigurationExportInputs<StepInputs>(inputSchemaProperties: InputProperty[], runtimeInputs: ObjectRuntimeInputs<StepInputs>, octopusServerEnricher: OctopusServerEnricher): ObjectStepConfigurationExportInputs<StepInputs>;
